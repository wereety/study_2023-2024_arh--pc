     1                                  ; lab4.asm
     2                                  SECTION .data 				; Начало секции данных
     3 00000000 D0A0D0B0D0B4D0BDD0-     	hello: DB 'Раднаев Ардан',10 	; 'Hello world!' плюс
     3 00000009 B0D0B5D0B220D090D1-
     3 00000012 80D0B4D0B0D0BD0A   
     4                                  					; символ перевода строки
     5                                  	helloLen: EQU $-hello 		; Длина строки hello
     6                                  	
     7                                  SECTION .text 		; Начало секции кода
     8                                  	GLOBAL _start
     9                                  	
    10                                  _start: 		; Точка входа в программу
    11 00000000 B804000000              mov eax,4 		; Системный вызов для записи (sys_write)
    12 00000005 BB01000000              mov ebx,1 		; Описатель файла '1' - стандартный вывод
    13 0000000A B9[00000000]            mov ecx,hello		; Адрес строки hello в ecx
    14 0000000F BA1A000000              mov edx,helloLen 	; Размер строки hello
    15 00000014 CD80                    int 80h 		; Вызов ядра
    16 00000016 B801000000              mov eax,1 		; Системный вызов для выхода (sys_exit)
    17 0000001B BB00000000              mov ebx,0 		; Выход с кодом возврата '0' (без ошибок)
    18 00000020 CD80                    int 80h 		; Вызов ядра
